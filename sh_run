---
- name: Obtener_info
  hosts: all
  gather_facts: false
  connection: network_cli

  tasks:
  - name: vty
    ios_command:
      commands:

        - show run | inc line vty
        - show run | inc vrf INTERNET

    register: get_vty_info

  - name: imprimimos la info
    debug:
      var: get_vty_info
